{
    "name": "alifahmmed/helpers-package",
    "type": "library",
    "description": "A Laravel helper package for file uploads, slug generation, and JSON responses",
    "keywords": ["helper", "package", "laravel", "file upload", "slug generation", "json responses"],
    "homepage": "https://github.com/s-m-alif-ahmmed/helpers-package",
    "license": "MIT",
    "version": "1.1.0",
    "authors": [
        {
            "name": "S M Alif Ahmmed",
            "email": "smalifahmmed@gmail.com",
            "homepage": "https://smalifahmmed.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "AlifAhmmed\\HelperPackage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlifAhmmed\\HelperPackage\\HelperServiceProvider"
            ],
            "aliases": {
                "Helper": "AlifAhmmed\\HelperPackage\\Helpers\\Helper"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
